<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item n1:id="@android:id/background"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <corners n1:radius="4.0dip" />
            <solid n1:color="@color/topicProgressBg" />
        </shape>
    </item>
    <item n2:id="@android:id/progress"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <corners n2:radius="4.0dip" />
                <solid n2:color="@color/topicProgressSecond" />
            </shape>
        </clip>
    </item>
    <item n3:id="@android:id/secondaryProgress"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <corners n3:radius="4.0dip" />
                <solid n3:color="@color/topicProgressFirst" />
            </shape>
        </clip>
    </item>
</layer-list>